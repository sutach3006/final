<link rel="stylesheet" href="/index.css">
<body>
    <h1 class="cta">Contact</h1>
    <div class="flexboxcta">
        {{#with data}}
            <div class="boxcta">
                <li class="ctn">
                    <span class="first-block">Facebook &nbsp: &nbsp&nbsp</span><span class="second-block"> {{contact.fb}}</span>
                </li>
            </div>
            <div class="boxcta">
                <li class="ctn">
                    <span class="first-block">Instagram &nbsp: &nbsp&nbsp</span><span class="second-block"> {{contact.ig}}</span>
                </li>
            </div>
            <div class="boxcta">
                <li class="ctn">
                    <span class="first-block">Line ID &nbsp: &nbsp&nbsp</span><span class="second-block"> {{contact.line}}</span>
                </li>
            </div>
            <div class="boxcta">
                <li class="ctn">
                    <span class="first-block">Email &nbsp: &nbsp&nbsp</span><span class="second-block">
                        <a class="email" href="mailto:{{contact.email}}"> {{contact.email}}</a>
                    </span>
                </li>
            </div>
             <div class="boxcta">
                 <li class="ctn">
                    <span class="first-block">Phone number &nbsp: &nbsp&nbsp</span><span class="second-block"> {{contact.phone}}</span>
                </li>
             </div>
        {{/with}}
    </div>
</body>